{
  "ci": {
    "collect": {
      "staticDistDir": "dist"
    },
    "assert": {
      "assertions": {
        "categories:performance": [
          "error",
          {
            "minScore": 0.9
          }
        ],
        "categories:accessibility": [
          "error",
          {
            "minScore": 0.9
          }
        ],
        "script-treemap-data": "off",
        "total-byte-weight": [
          "warn",
          {
            "maxNumericValue": 300000
          }
        ],
        "unused-javascript": [
          "warn",
          {
            "maxNumericValue": 250000
          }
        ]
      }
    }
  }
}